# 1. docker build -t mysso:0.0.1 -f .\sso_dockerfile .
# 2. "eval $(minikube docker-env)" for unix-like, "minikube docker-env | Invoke-Expression"  for powershell
FROM openjdk:17-jdk
WORKDIR /sso
ENV SPRING_PROFILES_ACTIVE=k8s
COPY ./target/sso-0.0.1-SNAPSHOT.jar app.jar
ENTRYPOINT ["java","-jar","/sso/app.jar"]